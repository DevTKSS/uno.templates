{
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:8080",
      "sslPort": 0
    }
  },
	"profiles": {
//#if (!useMsalAuthentication)
		"MyExtensionsApp.1 (WinAppSDK Unpackaged)": {
			"commandName": "Project"
		},
//#endif
		"MyExtensionsApp.1 (WinAppSDK Packaged)": {
			"commandName": "MsixPackage"
		},
//#if (useDesktop)
		"MyExtensionsApp.1 (Desktop)": {
			"commandName": "Project"
		},
    "MyExtensionsApp.1 (Desktop WSL2)": {
      "commandName": "WSL2",
      "commandLineArgs": "{ProjectDir}/bin/Debug/$(DotNetVersion)-desktop/MyExtensionsApp.1.dll",
      "distributionName": ""
    },
//#endif
//#if (useWasm)
    "MyExtensionsApp.1 (Wasm)": {
      "commandName": "Project",
      "dotnetRunMessages": true,
      "launchBrowser": true,
      "applicationUrl": "http://localhost:5001",
      "inspectUri": "{wsProtocol}://{url.hostname}:{url.port}/_framework/debug/ws-proxy?browser={browserInspectUri}",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "MyExtensionsApp.1 (Wasm - IIS Express)": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "inspectUri": "{wsProtocol}://{url.hostname}:{url.port}/_framework/debug/ws-proxy?browser={browserInspectUri}",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    }
//#endif
	}
}
