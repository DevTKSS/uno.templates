      "type": "parameter",
      "datatype": "choice",
      "enableQuotelessLiterals": true,
      "replaces": "$baseTargetFramework$",
      "defaultValue": "net8.0",
      "description": "Select the .NET version of your solution",
      "choices": [
        {
          "choice": "net8.0",
          "displayName": ".NET 8.0",
          "description": "Target .NET 8.0 (Long Term Support)"
        },
        {
          "choice": "net9.0",
          "displayName": ".NET 9.0",
          "description": "Target .NET 9.0 (Preview)"
        }
      ]
    },
    "unoWinUIVersion": {
      "type": "parameter",
      "datatype": "text",
      "defaultValue": "DefaultUnoWinUIVersion",
      "replaces": "$UnoWinUIVersion$"
    }
  },
  "primaryOutputs": [
    {
      "path": "UnoCrossRuntimeLib\\UnoCrossRuntimeLib.csproj"
    },
    {
      "path": "UnoCrossRuntimeLib\\UnoCrossRuntimeLib.Wasm.csproj"
    },
    {
      "path": "UnoCrossRuntimeLib\\UnoCrossRuntimeLib.Skia.csproj"
    }
  ],
